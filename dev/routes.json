[{
    "id": "http-all",
    "disabled": true,
    "source_url": "^http://(?P<1>.*)$",
    "destination_url": "https://${1}",
    "priority": 1010,
    "redirect_status": 308
},{
    "id": "www-all",
    "source_url": "^https://www\\.(?<1>.*)$",
    "destination_url": "https://${1}",
    "priority": 1000,
    "redirect_status": 308
},{
    "id": "hello-world",
    "source_url": "^https?://localhost(?::[0-9]+)?/hello$",
    "destination_url": "/world"
},{
    "id": "hello-world-number",
    "source_url": "^https?://localhost(?::[0-9]+)?/hello/(?<1>[0-9]+)$",
    "destination_url": "/world/${1}"
},{
    "id": "example-http-redirect",
    "source_url": "^http://(?P<1>example\\.net:8080/.*)$",
    "destination_url": "https://${1}",
    "priority": 1010,
    "redirect_status": 308
},{
    "id": "example-priority-low",
    "source_url": "^https://example\\.net:8443/test$",
    "destination_url": "/a",
    "priority": 100
},{
    "id": "example-priority-high",
    "source_url": "^http://example\\.net:8443/test$",
    "destination_url": "/b",
    "priority": 200
},{
    "id": "example-permanent-redirect",
    "source_url": "^http://example\\.net:8443/test2$",
    "destination_url": "/test3",
    "redirect_status": 308
},{
    "id": "example-discord",
    "source_url": "^https://example\\.net:8443/discord$",
    "destination_url": "https://discord.com/invite/X"
}]
